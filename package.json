{
  "name": "roverz-container-video-conference",
  "version": "0.0.1",
  "description": "video conference container",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "pretest": "./node_modules/.bin/eslint -c .eslintrc --ext .js src",
    "test": "jest"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/elarasu/roverz-container-video-conference.git"
  },
  "keywords": [
    "video conferencing",
    "container"
  ],
  "author": "elarasu",
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|tcomb-form-native|apsl-react-native-button,react-native-device-info|react-clone-referenced-element)"
    ],
    "collectCoverage": true,
    "verbose": true
  },
  "dependencies": {
    "jitsi-meet-logger": "github:elarasu/jitsi-meet-logger",
    "lib-jitsi-meet": "github:elarasu/lib-jitsi-meet",
    "react-native-webrtc": "github:elarasu/react-native-webrtc",
    "strophe": "1.2.4",
    "strophejs-plugins": "0.0.7"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "2.1.0",
    "babel-register": "^6.16.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-react-native": "3.0.1",
    "jest": "^20.0.4",
    "jest-react-native": "^18.0.0",
    "react-test-renderer": "^16.0.0-alpha.6",
    "string-replace-loader": "^1.2.0",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.7.1"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
